<p id="notice"><%= notice %></p>

<% content_for :top_content do %>
  <div class="jumbotron">
    <div class="container">
      <h2>Are you dissatisfied with the leading Presidential candidates?</h2>
      <p>Register to get started.</p>
      <p>
        <%= link_to 'Yes', new_user_registration_path, class: 'btn btn-primary btn-lg' %>
        <%= link_to 'No', new_user_registration_path, class: 'btn btn-primary btn-lg' %>
      </p>
    </div>
  </div>
<% end %>

<h1>Listing Landings</h1>

<table>
  <thead>
    <tr>
      <th>Dissatisfied</th>
      <th>Preferance</th>
      <th>Do something</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @landings.each do |landing| %>
      <tr>
        <td><%= landing.dissatisfied %></td>
        <td><%= landing.preferance %></td>
        <td><%= landing.do_something %></td>
        <td><%= landing.other %></td>
        <td><%= link_to 'Show', landing %></td>
        <td><%= link_to 'Edit', edit_landing_path(landing) %></td>
        <td><%= link_to 'Destroy', landing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Landing', new_landing_path %>
